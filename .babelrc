{
  "presets": ["react", "es2015", "stage-2"],

  "plugins": [
    "add-module-exports",
    "transform-decorators-legacy",
    "transform-react-display-name",
    "transform-class-properties",
    "syntax-decorators"
  ],

  "env": {
    "development": {
      "plugins": [
        "typecheck"
      ]
    }
  }
}
