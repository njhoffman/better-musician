{
  "name": "instrumental",
  "version": "1.0.29",
  "description": "Instrumental App",
  "main": "index.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "postinstall": "./bin/init-hooks",
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "compile:prod": "better-npm-run compile:prod",
    "lint": "./node_modules/.bin/esw bin config server src tests --color",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch --clear",
    "lint:watch:changed": "npm run lint -- --changed --watch --clear",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:watch": "npm run test -- --watch",
    "test:integration": "better-npm-run integration",
    "test:nightwatch": "better-npm-run nightwatch",
    "coverage": "better-npm-run coverage",
    "coverage:watch": "npm run coverage",
    "deploy": "better-npm-run deploy -- --watch",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "coveralls": "cat coverage/*/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "stats": "webpack --env development --profile --json > stats.json",
    "gitstats": "gitstats ./.git ./reports/gitstats",
    "notes": "notes config/ server/ src/",
    "e2e-setup": "./node_modules/selenium-standalone/bin/selenium-standalone install",
    "plato": "./node_modules/.bin/es6-plato -r -d report -t src shared server",
    "cosmos": "cosmos --config config/cosmos.js",
    "cosmos:export": "cosmos-export --config config/cosmos.js"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "compile:prod": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "nodemon --config ./nodemon.json bin/dev-server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "NODE_ENV": "production",
        "PORT": 4000,
        "API_PORT": 4001,
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "coverage": {
      "command": "node ./node_modules/karma/bin/karma start config/karma",
      "env": {
        "NODE_ENV": "test",
        "NODE_ENV_FLAG": "verbose",
        "DEBUG": "app:*"
      }
    },
    "nightwatch": {
      "command": "nightwatch --env local",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "integration": {
      "_command": "./node_modules/.bin/cypress open --config integrationFolder=tests/integration/specs fixturesFolder=tests/integration/fixtures pluginsFile=tests/integration/plugins.js supportFile=tests/integration/support.js videosFolder=tests/integration/videos screenShotsFolder=tests/integration/screenshots",
      "command": "./node_modules/.bin/cypress open --config browser=chrome",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/njhoffman/instrumental.git"
  },
